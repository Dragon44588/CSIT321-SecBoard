{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport api from \"@/api/APIs\";\nexport default {\n  __name: 'Delete-requests',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const mytoken = window.sessionStorage.getItem(\"token\");\n    const deleteReuqestsList = ref();\n    const authForm = reactive({\n      token: mytoken\n    });\n    api.getDeleteRequest(authForm).then(res => {\n      deleteReuqestsList.value = res.deletion_requests;\n    });\n    function get_delete_popup_content(index) {\n      showPopup_Delete.value = true;\n      delete_popup.value = deleteReuqestsList.value[index];\n    }\n    const handle_delete_request_form = reactive({\n      token: mytoken,\n      post_id: null,\n      yes_or_no: null\n    });\n    function handleDeleteRequest(post, yes_or_no) {\n      handle_delete_request_form.post_id = post.post_id;\n      handle_delete_request_form.yes_or_no = yes_or_no;\n      api.handleDeleteRequest(handle_delete_request_form).then(res => {\n        if (res.status === 200) {\n          console.log(\"handled a delete request\");\n          api.getDeleteRequest(authForm).then(res => {\n            deleteReuqestsList.value = res.deletion_requests;\n          });\n        }\n      });\n    }\n    const __returned__ = {\n      mytoken,\n      deleteReuqestsList,\n      authForm,\n      get_delete_popup_content,\n      handle_delete_request_form,\n      handleDeleteRequest,\n      reactive,\n      ref,\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","api","mytoken","window","sessionStorage","getItem","deleteReuqestsList","authForm","token","getDeleteRequest","then","res","value","deletion_requests","get_delete_popup_content","index","showPopup_Delete","delete_popup","handle_delete_request_form","post_id","yes_or_no","handleDe